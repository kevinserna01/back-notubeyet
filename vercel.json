{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node",
        "config": { "maxDuration": 30 }
      }
    ],
    "routes": [
      {
        "src": "/favicon.ico",
        "headers": { "status": "204" }
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  